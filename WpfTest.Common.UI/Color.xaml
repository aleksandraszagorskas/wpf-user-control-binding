    <SolidColorBrush x:Key="RAL_7040_SelectionColorBrush" Color="{StaticResource RAL_7040_SelectionColor}" />

    <SolidColorBrush x:Key="RAL_7024_SelectionColorBrush" Color="{StaticResource RAL_7024_SelectionColor}" />
    <SolidColorBrush x:Key="RAL_1015_SelectionColorBrush" Color="{StaticResource RAL_1015_SelectionColor}" />
    <SolidColorBrush x:Key="RAL_5010_SelectionColorBrush" Color="{StaticResource RAL_5010_SelectionColor}" />
    <SolidColorBrush x:Key="RAL_8017_SelectionColorBrush" Color="{StaticResource RAL_8017_SelectionColor}" />
    <SolidColorBrush x:Key="RAL_1021_SelectionColorBrush" Color="{StaticResource RAL_1021_SelectionColor}" />
    <SolidColorBrush x:Key="RAL_3011_SelectionColorBrush" Color="{StaticResource RAL_3011_SelectionColor}" />
    <SolidColorBrush x:Key="RAL_6020_SelectionColorBrush" Color="{StaticResource RAL_6020_SelectionColor}" />
</ResourceDictionary>